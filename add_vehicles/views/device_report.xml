<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_device_form" model="ir.ui.view">
        <field name="name">view.device.report.form</field>
        <field name="model">customer.device</field>
        <field name="inherit_id" ref="add_vehicles.device_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="before">
                <button name="device_report_excel" string="Print Device Report Excel" type="object"/>
            </xpath>
        </field>
    </record>
</odoo>
